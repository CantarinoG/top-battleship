{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-cycle":"off",
        "linebreak-style":"off",
        "import/no-mutable-exports":"off",
        "no-param-reassign":"off",
        "import/prefer-default-export":"off",
        "no-loop-func":"off",
        "no-undef":"warn"
    }
}
